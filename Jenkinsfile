pipeline {
    agent any

    stages {
        qa('Build') {
            steps {
                echo 'Building..'
            }
        }
        qa('Test') {
            steps {
                echo 'Testing..'
            }
        }
        qa('Deploy') {
            steps {
                echo 'Deploying....'
            }

        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }

}
